{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/build/main.build.py": {
      "runtime": "@vercel/python@4.6.0",
      "maxDuration": 60
    }
  },
  "devCommand": "cd frontend && yarn dev",
  "installCommand": "cd frontend && yarn install",
  "buildCommand": "cd frontend && yarn build",
  "rewrites": [
    {
      "source": "/_next/static/:match*",
      "destination": "/static/:match*"
    },
    {
      "source": "/_next/server/:match*",
      "destination": "/server/:match*"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/build/main.build.py"
    },
    {
      "source": "/inngest",
      "destination": "/api/build/main.build.py"
    },
    {
      "source": "/inngest/(.*)",
      "destination": "/api/build/main.build.py"
    },
    {
      "source": "/",
      "destination": "/server/app/index.html"
    },

    {
      "source": "/favicon.ico",
      "destination": "/server/app/favicon.ico"
    },
    {
      "source": "/not_found",
      "destination": "/server/app/not_found"
    },
    {
      "source": "/:path*",
      "destination": "/server/app/:path*"
    }
  ],
  "trailingSlash": false,
  "outputDirectory": "frontend/.next"
}